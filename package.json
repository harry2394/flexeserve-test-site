{
  "name": "northlink-nextjs-boilerplate",
  "description": "A production ready Next.js boilerplate with Typescript, Jest, React Testing Library, Express.js, Sass, Css, EnvConfig, Fetch, Reverse Proxy, Bundle Analyzer and Built-in Project CLI.  Starting point for all client NextJS projects.",
  "version": "2.0.0",
  "repository": "https://github.com/tbc",
  "author": "NorthLink Digital <info@northlink.digital>",
  "scripts": {
    "start:dev": "cross-env NODE_OPTIONS=\"-r next-logger\" nodemon",
    "build": "NODE_OPTIONS=\"-r next-logger\" next build && tsc --project tsconfig.server.json",
    "start": "NODE_OPTIONS=\"-r next-logger\" cross-env NODE_ENV=production node dist/index.js",
    "test": "jest --coverage",
    "lint": "eslint './{src,pages,server}/**/*' --ext .tsx,.ts",
    "pretty": "prettier --write --config .prettierrc.json {src,pages,server}/**/*.{ts,tsx}",
    "format": "npm run lint -- --fix",
    "analyze": "cross-env BUNDLE_ANALYZE=both npm run build",
    "export": "cross-env NODE_ENV=production next export",
    "cli": "pankod-cli add",
    "storybook": "start-storybook -s ./public/static -p 6006",
    "build-storybook": "build-storybook -s ./public/static  -c .storybook -o .storybookout"
  },
  "dependencies": {
